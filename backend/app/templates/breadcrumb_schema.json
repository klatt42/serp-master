{
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "Home",
      "item": "{{website_url}}"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "{{category_name}}",
      "item": "{{category_url}}"
    },
    {
      "@type": "ListItem",
      "position": 3,
      "name": "{{page_name}}",
      "item": "{{page_url}}"
    }
  ]
}
